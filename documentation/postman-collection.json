          "event": [
            {
              "listen": "prerequest",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "var username = pm.collectionVariables.get(\"username\");",
                  "var password = pm.collectionVariables.get(\"password\");",
                  "var baseUrl = pm.environment.get(\"URL\");",
                  "",
                  "pm.sendRequest({",
                  "    url: `${baseUrl}/v2/auth/login`,",
                  "    method: 'POST',",
                  "    header: 'Content-Type:application/json',",
                  "    body: {",
                  "        mode: 'raw',",
                  "        raw: JSON.stringify({ username, password })",
                  "    }",
                  "}, function (err, res) {",
                  "   if (err) {",
                  "       console.log(err);",
                  "   }",
                  "    const body = res.json();",
                  "    const token = body.token;",
                  "    const refreshToken = body.refreshToken;",
                  "",
                  "    // be so nice and revoke the refreshToken to not spam the database",
                  "    pm.sendRequest({",
                  "    url: `${baseUrl}/v2/auth/refresh`,",
                  "    method: 'DELETE',",
                  "    header: [",
                  "        'Content-Type:application/json',",
                  "        `Authorization:Bearer ${token}`    ",
                  "    ],",
                  "    body: {",
                  "        mode: 'raw',",
                  "        raw: JSON.stringify({ refreshToken })",
                  "    }",
                  "    },() => {}); ",
                  "",
                  "    const tokenBody = token.split(\".\")[1];",
                  "    const tokenBodyAsString = atob(tokenBody);",
                  "    const tokenDecoded = JSON.parse(tokenBodyAsString);",
                  "    pm.collectionVariables.set(\"userId\", tokenDecoded.id);",
                  "",
                  "    pm.collectionVariables.set(\"token\", \"Bearer \" + token);",
                  "});"
                ]
              }
            },
            {
              "listen": "test",
              "script": {
                "type": "text/javascript",
